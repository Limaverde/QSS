/* QDarkStyleSheet -----------------------------------------------------------
See Qt documentation:
  - https://doc.qt.io/qt-5/stylesheet.html
  - https://doc.qt.io/qt-5/stylesheet-reference.html
  - https://doc.qt.io/qt-5/stylesheet-examples.html
--------------------------------------------------------------------------- */
/* QDialog ---------------------------------------------------------------- */
QDialog { background: #333538; }

/* QMainWindow ------------------------------------------------------------
This adjusts the splitter in the dock widget, not qsplitter
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qmainwindow
--------------------------------------------------------------------------- */
QMainWindow::separator { }
QMainWindow::separator:hover { }
QMainWindow::separator:horizontal { }
QMainWindow::separator:vertical { }

/* QSizeGrip --------------------------------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qsizegrip
--------------------------------------------------------------------------- */
QSizeGrip { background: transparent; width: 12px; height: 12px; image: url(""); }

/* QSplitter --------------------------------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qsplitter
--------------------------------------------------------------------------- */
QSplitter { }
QSplitter::handle { background-color: none; image: url(":/images/svg_icons/dark/splitter_v_handle.svg");  }
QSplitter::handle:horizontal { width: 4px; }
QSplitter::handle:vertical { height: 4px; }

/* QStackedWidget -------------------------------------------------------- */
QStackedWidget { padding: 0px; }

/* Workaround to fix Hyperlink dialog wrap box */
QStackedWidget QWidget[styleclass='fixBorder'] { border: 1px solid #1e2023; }
QStackedWidget QPushButton[styleclass='fixHeight'] { height: 18px; }
/* --- */

/* QDockWiget -------------------------------------------------------------
--------------------------------------------------------------------------- */
QDockWidget
{
  outline: 1px solid #32414B;
  background-color: #19232D;
  border: 1px solid #32414B;
  border-radius: 4px;
  titlebar-normal-icon: url("");
}
QDockWidget::title { padding: 6px; spacing: 4px; border: none; background-color: #32414B; }
QDockWidget::close-button {  background-color: #32414B; border-radius: 4px; border: none; }
QDockWidget::close-button:hover { image: url(""); }
QDockWidget::close-button:pressed { image: url(""); }
QDockWidget::float-button { background-color: #32414B; border-radius: 4px; border: none; }
QDockWidget::float-button:hover { image: url(""); }
QDockWidget::float-button:pressed { image: url(""); }

/* QStatusBar -------------------------------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qstatusbar
--------------------------------------------------------------------------- */
QStatusBar { border: 1px solid #32414B; background: #32414B; }
QStatusBar::item { border: none; }
QStatusBar QToolTip
{
  background: #148CD2;
  border: 1px solid #19232D;
  color: #19232D;
  padding: 0px;
  opacity: 230;
}
QStatusBar QLabel { background: transparent; }

/* QProgress Bar ---------------------------------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qprogressbar
--------------------------------------------------------------------------- */
QProgressBar { border: 1px solid #000; border-radius: 0px; text-align:center; color: #fff; background: #2d2f31; }
QProgressBar::chunk { background-color: #141517; width: 20px; }

/* QToolTip ---------------------------------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qtooltip
--------------------------------------------------------------------------- */
QToolTip
{
  background: #d4d4d4;
  border: 1px solid #1e2023;
  color: #000000;
  border-radius: 1px;
}

/* QMenu ------------------------------------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qmenu
--------------------------------------------------------------------------- */
QMenu {
    border: 1px solid #000;
    color: #cfcfcf;
    margin: 0px;
    padding: 3px;
    background: #333538;
}
QMenu::separator { height: 1px; background: #1e2023; }
QMenu::icon { margin: 0px; padding-left: 8px; }
QMenu::icon:checked { margin: 0px; padding-left: 8px; image: url(":/images/svg_icons/dark/checkbox_check.svg"); }
QMenu::item { padding: 4px 24px 4px 24px; border: 1px transparent #32414B; }
QMenu::item:hover { }
QMenu::item:selected { color: #fff; background: #141518;  }
QMenu::item:disabled { color: #646464; }
QMenu::indicator { width: 12px; height: 12px; padding-left: 6px; image: url(":/images/svg_icons/dark/checkbox_check.svg");
  /* non-exclusive indicator = check box style indicator (see QActionGroup::setExclusive) */
  /* exclusive indicator = radio button style indicator (see QActionGroup::setExclusive) */
}
QMenu::indicator:non-exclusive:unchecked { image: url(""); }
QMenu::indicator:non-exclusive:unchecked:selected { image: url(""); }
QMenu::indicator:non-exclusive:checked { image: url(""); }
QMenu::indicator:non-exclusive:checked:selected { image: url(""); }
QMenu::indicator:exclusive:unchecked { image: url(""); }
QMenu::indicator:exclusive:unchecked:selected { image: url(""); }
QMenu::indicator:exclusive:checked { image: url(""); }
QMenu::indicator:exclusive:checked:selected { image: url(""); }
QMenu::right-arrow { margin: 5px; image: url(":/images/svg_icons/dark/arrow_right.svg"); height: 12px; width: 12px; }

/* -------  Workaround for Print Preview Dialog RMB buttons looks like in Light UI  ----------------------- */
QMenu[styleclass="customQMenuIcon"]::item { padding: 4px 24px 4px 35px; border: 1px transparent #646464; }
QMenu[styleclass="customQMenuIcon"]::indicator {  left: 0px; }
QMenu[styleclass="customQMenuIcon"]::indicator:non-exclusive:unchecked { margin: 0px; padding-left: 0px; left: 0px; border: transparent;}
QMenu[styleclass="customQMenuIcon"]::indicator:non-exclusive:checked { left: 0px; border: 1px solid #646464; }
QMenu[styleclass="customQMenuIcon"]::indicator:exclusive:checked { left: 1px; border: 1px solid #646464; height: 20px; width: 18px; }
QMenu[styleclass="customQMenuIcon"]::icon:checked { position: absolute; top: 0px; left: -25px; right: -10px; }
QMenu[styleclass="customQMenuIcon"]::icon:unchecked { position: absolute; top: 1px; right: 1px; bottom: 1px; left: -20px; }
/* ----------------------------------------------------------------------------------------------------- */

/* QCheckBox --------------------------------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qcheckbox
--------------------------------------------------------------------------- */
QCheckBox
{
  color: #cfcfcf;
  spacing: 5px;
  background: none;
}
QCheckBox:disabled { color: #646464; }
QCheckBox:hover { color: #fff; }
QCheckBox:focus { }
QCheckBox QWidget:disabled { }

QCheckBox::indicator { height: 12px; width: 12px; border: 1px solid #646464; background:#000; border-radius: 2px; }

QCheckBox::indicator:unchecked { }
QCheckBox::indicator:unchecked:hover { border: 1px solid #284c93; }
QCheckBox::indicator:unchecked:focus { }
QCheckBox::indicator:unchecked:pressed { }
QCheckBox::indicator:unchecked:disabled { border: 1px solid #646464; background:#646464; }

QCheckBox::indicator:checked { image:url(":/images/svg_icons/dark/checkbox_check.svg"); }
QCheckBox::indicator:checked:hover { border: 1px solid #284c93; }
QCheckBox::indicator:checked:focus,
QCheckBox::indicator:checked:pressed {  }
QCheckBox::indicator:checked:disabled { border: 1px solid #646464; background:#646464; }

QCheckBox::indicator:indeterminate:disabled { border: 1px solid #646464; }
QCheckBox::indicator:indeterminate:focus,
QCheckBox::indicator:indeterminate:hover,
QCheckBox::indicator:indeterminate:pressed { }

QCheckBox[styleclass="titleCheckBox"] { border-bottom: 2px solid #1E2023; }

/* QGroupBox --------------------------------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qgroupbox
--------------------------------------------------------------------------- */
QGroupBox,
QStackedWidget > QWidget > QGroupBox
{
  border-top: 2px solid #1E2023;
  font: bold;
  font-size: 14px;
  margin-top: 18px;
  color: #cfcfcf;
  padding: 10px 0;
}
QGroupBox[styleclass="noTopMargin"],
QStackedWidget > QWidget > QGroupBox[styleclass="noTopMargin"] { margin-top: 0px; }

QGroupBox[styleclass="noPadding"],
QStackedWidget > QWidget > QGroupBox[styleclass="noPadding"] { padding: 0px; }

QGroupBox::title { subcontrol-origin: margin;subcontrol-position:top left; left:0; padding:0; }
QGroupBox::indicator {   }
QGroupBox::indicator:unchecked { border: 1px solid #000; }
QGroupBox::indicator:unchecked:hover,
QGroupBox::indicator:unchecked:focus,
QGroupBox::indicator:unchecked:pressed { image: url(""); }
QGroupBox::indicator:unchecked:disabled { image: url(""); }

QGroupBox::indicator:checked { image:url(":/images/svg_icons/dark/checkbox_check.svg"); }
QGroupBox::indicator:checked:hover,
QGroupBox::indicator:checked:focus,
QGroupBox::indicator:checked:pressed { image: url(""); }
QGroupBox::indicator:checked:disabled { image: url(""); }

/* QRadioButton -----------------------------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qradiobutton
--------------------------------------------------------------------------- */
QRadioButton
{
  background: none;
  color: #CFCFCF;
  spacing: 4px;
  padding: 0px;
  margin: 0px;
  border: none;
  outline: none;
  margin-left: -1px;
}
QRadioButton:focus {   }
QRadioButton:disabled { color: #646464; }
QRadioButton:hover { color: #fff; }
QRadioButton::indicator {   }
QRadioButton::indicator:unchecked { image:url(":/images/svg_icons/dark/radio_uncheck.svg"); }
QRadioButton::indicator:unchecked:hover { image:url(":/images/svg_icons/dark/radio_uncheck_hover.svg"); }
QRadioButton::indicator:unchecked:focus,
QRadioButton::indicator:unchecked:pressed { }
QRadioButton::indicator:unchecked:disabled { image:url(":/images/svg_icons/dark/radio_uncheck_disable.svg"); }
QRadioButton::indicator:checked { image:url(":/images/svg_icons/dark/radio_check.svg"); }
QRadioButton::indicator:checked:hover { image:url(":/images/svg_icons/dark/radio_check_hover.svg"); }
QRadioButton::indicator:checked:focus,
QRadioButton::indicator:checked:pressed {  }
QRadioButton::indicator:checked:disabled { image:url(":/images/svg_icons/dark/radio_check_disable.svg"); }

/* Style created to fix issues when radio are in line */
QRadioButton[styleclass="inLineRadio"] { margin-right: 6px; }

/* QScrollBar -------------------------------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qscrollbar
--------------------------------------------------------------------------- */
/* VERTICAL -------------------- */
QScrollBar:vertical
{
  border: 1px solid #000;
  background: none;
  background-color: transparent;
  width: 18px;
  margin: 18px 0 18px 0;
}
QScrollBar::handle:vertical { background: #333538; min-height: 32px; min-width: 18px; }
QScrollBar::add-line:vertical
{
  border: 1px solid #000;
  height: 17px;
  background: url(":/images/svg_icons/dark/arrow_down.svg")  center center no-repeat;
  background-color: #000;
  subcontrol-position: bottom;
  subcontrol-origin: margin;
}
QScrollBar::sub-line:vertical
{
  border: 1px solid #000;
  height: 17px;
  background: url(":/images/svg_icons/dark/arrow_up.svg") center center no-repeat;
  background-color: #000;
  subcontrol-position: top;
  subcontrol-origin: margin;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical { background: #000; }
QScrollBar::handle:vertical:pressed,
QScrollBar::add-line:vertical:pressed,
QScrollBar::sub-line:vertical:pressed { background-color: #646464; }

/* HORIZONTAL POSITION -------------------- */
QScrollBar:horizontal
{
  border: 1px solid #000;
  background: none;
  background-color: transparent;
  height: 18px;
  margin: 0 18px 0 18px;
}
QScrollBar::handle:horizontal { background: #333538; min-width: 8px; }
QScrollBar::add-line:horizontal
{
  border: 1px solid #000;
  width: 18px;
  background: url(":/images/svg_icons/dark/arrow_right.svg")  center center no-repeat;
  background-color: #000;
  subcontrol-position: right;
  subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal
{
  border: 1px solid #000;
  width: 18px;
  background: url(":/images/svg_icons/dark/arrow_left.svg") center center no-repeat;
  background-color: #000;
  subcontrol-position: left;
  subcontrol-origin: margin;
}
QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal { background: #000; }

QScrollBar::handle:horizontal:pressed,
QScrollBar::add-line:horizontal:pressed,
QScrollBar::sub-line:horizontal:pressed { background-color: #646464; }

/* QTextEdit --------------------------------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-specific-widgets
--------------------------------------------------------------------------- */
QTextEdit
{
  background: #000;
  color: #cfcfcf;
  border-radius: 0px;
  border: 1px solid #4A4A4A;
}
QTextEdit:hover { }
QTextEdit:focus { }
QTextEdit:selected { }
QTextEdit:disabled { color: #cfcfcf; }

/* Workaround for Advanced Attribute Edit Dialog */
QTableWidget QTextEdit[styleclass='noBorder'] { border: 0px; }
/* --- */

/* QPlainTextEdit ---------------------------------------------------------
--------------------------------------------------------------------------- */
QPlainTextEdit
{
  background-color: #000;
  color: #cfcfcf;
  border-radius: 0;
  border: 1px solid #4a4a4a;
}
QPlainTextEdit:hover {  }
QPlainTextEdit:focus {  }
QPlainTextEdit:selected {  }

/* QAbstractSpinBox -------------------------------------------------------
--------------------------------------------------------------------------- */
QAbstractSpinBox
{
  border: 1px solid #4a4a4a;
  color: #cfcfcf;
  padding-top: 2px;
  padding-bottom: 2px;
  padding-left: 4px;
  border-radius: 1px;
  min-height: 16px;
  background: #000;
}

/* Workaround to make spinboxes looks like an input (????) -------- */
/* Dialog example: Patternhatch, breakline  -------- */
QAbstractSpinBox[buttonSymbols="2"] { padding: 0px; padding-right: -20px; }
QAbstractSpinBox:hover {  border: 1px solid #284c93; }
QAbstractSpinBox:disabled {  background: #646464; color: #000; }
QAbstractSpinBox:up-button
{
  subcontrol-origin: border;
  subcontrol-position: top right;
  border: none;
  border-left: 1px solid #4a4a4a;
  height: 11px;
  width: 16px;
  background:rgba(204, 204, 204, 60);
  margin: 1px 1px 0 0;
}
QAbstractSpinBox:up-button:hover,
QAbstractSpinBox:down-button:hover { background:rgba(204, 204, 204, 60); }
QAbstractSpinBox:up-button:pressed,
QAbstractSpinBox:down-button:pressed { background:rgba(204, 204, 204, 20); }
QAbstractSpinBox::up-arrow { image: url(":/images/svg_icons/dark/arrow_up.svg"); height: auto; width: 10px; margin: -1px 0 0 -1px; }
QAbstractSpinBox::up-arrow:disabled,
QAbstractSpinBox::up-arrow:off { image: url(":/images/svg_icons/dark/arrow_up_disable.svg"); }
QAbstractSpinBox::up-arrow:hover { }
QAbstractSpinBox:down-button {
  subcontrol-origin: border;
  subcontrol-position: bottom right;
  border: none;
  border-left: 1px solid #4a4a4a;
  border-top: 1px solid #4a4a4a;
  height: 9px;
  width: 16px;
  background:rgba(204, 204, 204, 60);
  margin: 1px 1px 1px 0;
}
QAbstractSpinBox::down-arrow { image: url(":/images/svg_icons/dark/arrow_down.svg"); height: auto; width: 10px; margin: 0 0 1px 0; }
QAbstractSpinBox::down-arrow:disabled,
QAbstractSpinBox::down-arrow:off { image: url(":/images/svg_icons/dark/arrow_down_disable.svg"); }
QAbstractSpinBox::down-arrow:hover { }
QAbstractSpinBox:hover {  color: #fff; }
QAbstractSpinBox:focus {  }
QAbstractSpinBox:selected { color: #fff; }

/* QLabel -----------------------------------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qlabel
--------------------------------------------------------------------------- */
QLabel { color: #cfcfcf; }
QLabel:disabled { background-color: none; color: #646464; }

/* CFXTitleLabels ------------------------------------------------------- */
QLabel[styleclass="titleLabel"] { border-bottom: 2px solid #1E2023; padding-bottom:2px; font-size: 14px; }
a, QLabel a, QTextInput a { color: #fff; text-decoration: underline; }

/* QTextBrowser -----------------------------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qabstractscrollarea
--------------------------------------------------------------------------- */
QTextBrowser
{
  background-color: #000;
  border: 1px solid #4a4a4a;
  color: #cfcfcf;
  border-radius: 0px;
}
QTextBrowser:disabled { background-color: #646464; border: 1px solid #646464; color: #000; }
QTextBrowser:hover { }
QTextBrowser:!hover, QTextBrowser:selected, QTextBrowser:pressed { }

/* QPushButton ------------------------------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qpushbutton
--------------------------------------------------------------------------- */
QGroupBox > QPushButton,
QPushButton
{
  border: 1px solid #4a4a4a;
  background-color: #141518;
  border-radius: 0px;
  color: #cfcfcf;
  padding: 2px;
  min-height: 14px;
  min-width: 70px;
}
QPushButton::menu-indicator { subcontrol-origin: padding; subcontrol-position: bottom right; bottom: 4px; }

QPushButton:default { border: 1px solid #284c93; background: #333538; }
QPushButton:hover { color: #fff; border: 1px solid #284c93; background: #1E2023; }
QPushButton:disabled { background: #646464;  border: 1px solid #646464; color: #3D3D3D; }
QPushButton:focus { border: 1px solid #284c93; background: #1E2023; }
QPushButton:checked { border: 1px solid #1E2023; background: #434343; }
QPushButton:checked:focus { border: 1px solid #1E2023; background: #434343; }
QPushButton:checked:disabled { border: 1px solid #CFCFCF; color: #000; }

/* Custom Button Styles */
QPushButton[styleclass="calcButton"] { min-width: auto; }
QPushButton[styleclass="autoWidth"] { min-width: auto; }

/* QDialogButtonBox  ----------------------------*/
QDialogButtonBox > QPushButton
{
  background: #1e2023;
  border: 1px solid #4a4a4a;
  border-radius: 1px;
  padding: 4px 8px;
  min-width: 70px;
}
QDialogButtonBox > QPushButton:disabled { background: #333538; border: 1px solid #646464; color: #000; }
QDialog[class="CFxDialog"] > QPushButton:disabled { background: none; border: 1px solid #646464; color: #000; }

 /* QDialogBox ---
QDialogButtonBox {
    dialogbuttonbox-buttons-have-icons: 1;
    button-layout: 0;  Not Defined=System  /  0=Windows  /  1=Mac   /  2=KDE   /   3=Gnome
}
QPushButton[styleclass="buttonBoxOk"],
QPushButton[styleclass="buttonBoxApply"] {  }

QPushButton[styleclass="buttonBoxOk"]:disabled,
QPushButton[styleclass="buttonBoxApply"]:disabled {  }

QPushButton[styleclass="buttonBoxApply"]:disabled,
QPushButton[styleclass="buttonBoxOk"]:disabled {  }

QPushButton[styleclass="buttonBoxApply"]:hover,
QPushButton[styleclass="buttonBoxOk"]:hover { }

QPushButton[styleclass="buttonBoxHelp"] {  }
QPushButton[styleclass="buttonBoxCancel"] {  }
--- */

/* QToolButton ------------------------------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qtoolbutton
--------------------------------------------------------------------------- */
/* COMMON BUTTONS ----------------------------------------------------- */
QToolButton
{
  border: 1px solid #4a4a4a;
  background-color: #1e2023;
  border-radius: 0px;
  color: #cfcfcf;
  min-height: 22px;
  min-width: 22px;
  /* The subcontrols below are used only in the DelayedPopup mode */
  /* The subcontrols below are used only in the MenuButtonPopup mode */
  /* The subcontrol below is used only in the InstantPopup or DelayedPopup mode */
}
QToolButton:pressed { border: 1px solid #284c93; background: #000; }
QToolButton:disabled { color: #3d3d3d; background: #646464; }
QToolButton:hover { border: 1px solid #284c93; }
QToolButton:checked { border: 1px solid #284c93; background-color: #000; }
QToolButton:checked:disabled { }
QToolButton:focus { border: 1px solid #284c93; }
QToolButton:default { border: 1px solid #284c93; }

/* POPUP MODE ------ */
QToolButton[popupMode="0"] {  /* Only for DelayedPopup */ padding-right: 2px; }
QToolButton[popupMode="1"] { /* Only for MenuButtonPopup */ padding-right: 20px; }
QToolButton[popupMode="1"]::menu-button { border: none; }
QToolButton[popupMode="1"]::menu-button:hover { border: none; }
QToolButton[popupMode="2"] { /* Only for InstantPopup */ padding-right: 2px; }

QToolButton::menu-button { padding: 2px; border-radius: 4px; border: 1px solid #32414B; width: 12px; outline: none; }
QToolButton::menu-button:hover { }
QToolButton::menu-button:checked:hover {  }
QToolButton::menu-indicator { image: url(""); height: 8px; width: 8px; top: 0; left: -2px; }
QToolButton::menu-arrow {  image: url(":/images/svg_icons/dark/arrow_down.svg"); height: 8px; width: 8px; }
QToolButton::menu-arrow:hover { image: url(":/images/svg_icons/dark/arrow_down.svg"); }

/* --- Custom Classes */
QToolButton[styleclass="autoWidth"] { width: auto; }
/* ---- /

/* QToolBar  --- */
QToolBar { spacing: 0px; font: 11px; }
QToolBar::handle { }
QToolBar::separator { width: 1px; background: #cfcfcf; }
QToolBar QToolButton { min-width: 60px; min-height: 22px; padding: 0 5px; font: 11px; border: none; }

QToolBarExtension { background: url(":/images/svg_icons/dark/arrow_down.svg") left center no-repeat; }
QToolBarExtension:hover { border: 0px;  }
QToolBarExtension:selected { }

