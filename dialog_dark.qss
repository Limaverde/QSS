/* QComboBox --------------------------------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qcombobox
--------------------------------------------------------------------------- */
QComboBox,
QWidget > QGroupBox > QComboBox
{
  border: 1px solid #4a4a4a;
  padding: 0px 2px 2px 2px;
  min-height: 16px;
  background: #000;
  border-radius: 1px;
  color: #cfcfcf;
}

QComboBox:hover { border: 1px solid #284c93; }
QComboBox:focus { }
QComboBox:on { }
QComboBox:selected { background: #284c93; }
QComboBox:disabled { color: #000; background: #646464; }
QComboBox::indicator {  }
QComboBox::indicator:alternate { }
QComboBox::item:alternate {  }
QComboBox::item:checked {  }
QComboBox::item:selected {  }
QComboBox::drop-down { width: 16px; border-left: 1px solid #4a4a4a; background: #303030; }
QComboBox::drop-down:disabled { background: #646464; }
QComboBox::down-arrow { image: url(":/images/svg_icons/dark/arrow_down.svg"); height: 12px; width: 12px;  }
QComboBox::down-arrow:on,
QComboBox::down-arrow:hover,
QComboBox::down-arrow:focus { image: url(":/images/svg_icons/dark/arrow_down.svg"); }
QComboBox::down-arrow:disabled { image: url(":/images/svg_icons/dark/arrow_down_disable.svg"); }

/* QSlider ----------------------------------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qslider
--------------------------------------------------------------------------- */
QSlider {  }
QSlider:disabled {  }
QSlider:focus {  }

/* Horizontal --- */
QSlider::groove:horizontal { background: none; border: 1px solid #646464; height: 4px; margin: 2 0px; border-radius: 0px; padding: 0; }
QSlider::sub-page:horizontal { background: #000; height: 4px; margin: 0px; border-radius: 0px; }
QSlider::sub-page:horizontal:disabled { background: #646464; }
QSlider::handle:horizontal { background: #646464; border: 1px solid #adadad; width: 8px; height: 8px; margin: -8px 0; border-radius: 0px; }
QSlider::handle:horizontal:hover {  background: #adadad;  }
QSlider::handle:horizontal:focus { }

/* Vertical --- */
QSlider::groove:vertical { background: #32414B; border: 1px solid #32414B; width: 4px; margin: 0px; border-radius: 0px; }
QSlider::add-page:vertical { background: #1464A0; border: 1px solid #32414B; width: 4px; margin: 0px; border-radius: 4px; }
QSlider::add-page:vertical:disabled { background: #14506E; }
QSlider::handle:vertical { background: #787878; border: 1px solid #32414B; width: 8px; height: 8px; margin: 0px -8px; border-radius: 4px; }
QSlider::handle:vertical:hover { background: #148CD2; border: 1px solid #148CD2; }
QSlider::handle:vertical:focus { border: 1px solid #1464A0; }

/* QLineEdit --------------------------------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qlineedit
--------------------------------------------------------------------------- */
QWidget > QGroupBox > QLineEdit,
QLineEdit
{
  background: #000;
  padding: 2px 4px;
  border: 1px solid #4a4a4a;
  border-radius: 0px;
  color: #cfcfcf;
}
QLineEdit:disabled { background: #646464; color: #000; }
QLineEdit:hover { }
QLineEdit:focus { }
QLineEdit:selected { }

QLineEdit[styleclass="ga_input"] { background: #fff; }
QLineEdit[styleclass="ga_input"][error=false] { background: #fff; }
QLineEdit[error=true] { background: #960A0A; }


/* QTabWiget, QTabBar  --------------------------------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qtabwidget-and-qtabbar
--------------------------------------------------------------------------- */
QTabWidget { }
QTabWidget > QWidget {
  border-radius: 0px;
  border: 0px;
  padding: 0px;
  margin: 0px;
}
QTabWidget::tab-bar { top: 1px; }
QTabWidget::pane { border: 1px solid #646464; background: #4d4f52; }
QTabBar { qproperty-drawBase: 0; }
QTabBar::close-button { image: url(""); }
QTabBar::close-button:hover { image: url(""); }
QTabBar::close-button:pressed { image: url(""); }
QTabBar::tab { }

/* Top --- */
QTabBar::tab:top
{
  margin: 0px;
  padding: 2px 8px;
  min-width: 32px;
  border:1px solid #646464;
  color: #CFCFCF;
}
QTabBar::tab:top:selected:disabled {  }
QTabBar::tab:top:!selected:disabled {  }

QTabBar::tab:top:selected { background-color: #4d4f52; margin-bottom: -1px; }
QTabBar::tab:top:selected:hover { background-color: #4d4f52; }

QTabBar::tab:top:!selected {  margin-left: 0px; margin-top: 4px; background-color: #333538;  }
QTabBar::tab:top:!selected:hover {  background-color: #4d4f52; }

/* Right --- */
QTabBar::tab:right:!selected:disabled { }
QTabBar::tab:right:!selected { }
QTabBar::tab:right:selected:disabled { }

/* Bottom --- */
QTabBar::tab:bottom:selected:disabled { }
QTabBar::tab:bottom:!selected:disabled { }
QTabBar::tab:bottom:!selected { }

/* Left --- */
QTabBar::tab:left:selected:disabled { }
QTabBar::tab:left:!selected:disabled { }
QTabBar::tab:left:!selected { }

QTabBar::tab:left { }
QTabBar::tab:left:selected { }
QTabBar::tab:left:!selected:hover { }

QTabBar::tab:right { }
QTabBar::tab:right:selected { }
QTabBar::tab:right:!selected:hover { }

/* Any button inside the tab --- */
QTabBar QToolButton { background-color: #32414B; height: 12px; width: 12px; }
QTabBar QToolButton:pressed { background-color: #32414B; }
QTabBar QToolButton:pressed:hover { border: 1px solid #148CD2; }
QTabBar QToolButton::left-arrow:enabled { image: url(""); }
QTabBar QToolButton::left-arrow:disabled { image: url(""); }
QTabBar QToolButton::right-arrow:enabled { image: url(""); }
QTabBar QToolButton::right-arrow:disabled { image: url(""); }

/* QTreeView QListView QTableView -----------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qtreeview
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qlistview
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qtableview
--------------------------------------------------------------------------- */

/* QHeaderView ------------------------------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qheaderview
--------------------------------------------------------------------------- */
QHeaderView { padding: 0px; margin-right: 0px; height: 20px; border: none; }
QHeaderView:disabled { background-color: #646464; border: 1px transparent #32414B; padding: 1px; }

/* Horizontal */en
QHeaderView::section { border: none; }
QHeaderView::section::horizontal { color: #cfcfcf; background: #000; border: 1px solid #4a4a4a; padding: 0px 5px; }
QHeaderView::section::horizontal:selected,
QHeaderView::section::horizontal:checked {  }
QHeaderView::section::horizontal:disabled { }
QHeaderView::section::horizontal:hover { background: #333538; }
QHeaderView::up-arrow { width: 12px; image: url(":/images/svg_icons/dark/arrow_up.svg"); }
QHeaderView::down-arrow { width: 12px; image: url(":/images/svg_icons/dark/arrow_down.svg"); }

/* Vertical */
QHeaderView::section { border: none; }
QHeaderView::section::vertical { color: #cfcfcf; background: #000; border: 1px solid #4a4a4a; }
QHeaderView::section::vertical:selected,
QHeaderView::section::vertical:checked { background: #284c93;  }
QHeaderView::section::vertical:hover { background: #333538; }
QHeaderView::section::vertical:disabled { }

/* Hyperlinks(HTML) inside QLabel and QInputText --- */
QLabel > a { color: #cfcfcf; text-decoration: underline; }

/* General --- */
QTreeView {  }
QTreeView:branch {  }
QTreeView:branch:selected,
QTreeView:branch:hover { }

/* Branch --- */
QTreeView::branch:has-siblings:adjoins-item { background: url(":/images/svg_icons/dark/tree_open.svg") center center no-repeat; }
QTreeView::branch:has-siblings:!adjoins-item {  }
QTreeView::branch:has-siblings:adjoins-item { background: url("") center center repeat-y;  }
QTreeView::branch:!has-children:!has-siblings:adjoins-item {  }
QTreeView::branch:has-children:!has-siblings:closed,
QTreeView::branch:closed:has-children:has-siblings { background: url(":/images/svg_icons/dark/tree_open.svg") center center no-repeat;  }
QTreeView::branch:open:has-children:!has-siblings,
QTreeView::branch:open:has-children:has-siblings { background: url(":/images/svg_icons/dark/tree_close.svg") center center no-repeat; }
QTreeView::branch:has-children:!has-siblings:closed:hover,
QTreeView::branch:closed:has-children:has-siblings:hover { }
QTreeView::branch:open:has-children:!has-siblings:hover,
QTreeView::branch:open:has-children:has-siblings:hover { }

/* Item --- */
QTreeView::item { color: #cfcfcf; height: 20px; background: #1E2023; }
QTreeView::item:open,
QTreeView::item:closed { color: #cfcfcf; background: #1E2023; }
QTreeView::item:open:has-children:!has-siblings,
QTreeView::item:open:has-children:has-siblings { background: #1E2023; }

/* General List Styles  --- */
QTableView { gridline-color: #333538; color: #cfcfcf; border: 0px; alternate-background-color: #1E2023; }
QTableCornerButton::section { background: #333538; border: 1px transparent #000; }

QTreeView::item:pressed,
QListView::item:pressed,
QTableView::item:pressed,
QColumnView::item:pressed { }

QTableView:disabled { color: #646464; } /* this was solving no style for disable texts inside lists */

QTreeView::item:disabled,
QListView::item:disabled,
QTableView::item:disabled,
QColumnView::item:disabled { color: #cfcfcf; }

/* Active and Inactive Items --- */
QTreeView::item:selected:active,
QListView::item:selected:active,
QTableView::item:selected:active,
QColumnView::item:selected:active { background: #284c93; }
QTreeView::item:selected:!active,
QListView::item:selected:!active,
QTableView::item:selected:!active,
QColumnView::item:selected:!active { background: #284c93;  }

/* Disabled  Items --- */
QTreeView::item:selected:disabled,
QListView::item:selected:disabled,
QTableView::item:selected:disabled,
QColumnView::item:selected:disabled { background: #646464; }

/* Hover / Selected --- */
QTreeView::item:selected:hover,
QListView::item:selected:hover,
QTableView::item:selected:hover,
QColumnView::item:selected:hover { color: #fff; background: #284c93; }
QTreeView::item:!selected:hover,
QListView::item:!selected:hover,
QTableView::item:!selected:hover,
QColumnView::item:!selected:hover { color: #fff; background:#284c93; }

/* Checkbox inside Tables  --- */
QTableView::indicator { height: 12px; width: 12px; border: 1px solid #646464; background:#000; border-radius: 2px; }
QTableView::indicator:unchecked { }
QTableView::indicator:unchecked:hover { border: 1px solid #284c93; }
QTableView::indicator:unchecked:focus { }
QTableView::indicator:unchecked:pressed { }
QTableView::indicator:unchecked:disabled { border: 1px solid #646464; background:#646464; }
QTableView::indicator:checked { image:url(":/images/svg_icons/dark/checkbox_check.svg"); }
QTableView::indicator:checked:hover { border: 1px solid #284c93; }
QTableView::indicator:checked:focus,
QTableView::indicator:checked:pressed {  }
QTableView::indicator:checked:disabled { border: 1px solid #646464; background:#646464; }
QTableView::indicator:indeterminate:disabled { border: 1px solid #646464; }
QTableView::indicator:indeterminate:focus,
QTableView::indicator:indeterminate:hover,
QTableView::indicator:indeterminate:pressed { }

/* Checkbox inside TreeViews  --- */
QTreeView::indicator { height: 12px; width: 12px; border: 1px solid #646464; background:#000; border-radius: 2px; }
QTreeView::indicator:unchecked { }
QTreeView::indicator:unchecked:hover { border: 1px solid #284c93; }
QTreeView::indicator:unchecked:focus { }
QTreeView::indicator:unchecked:pressed { }
QTreeView::indicator:unchecked:disabled { border: 1px solid #646464; background:#646464; }
QTreeView::indicator:checked { image:url(":/images/svg_icons/dark/checkbox_check.svg"); }
QTreeView::indicator:checked:hover { border: 1px solid #284c93; }
QTreeView::indicator:checked:focus,
QTreeView::indicator:checked:pressed {  }
QTreeView::indicator:checked:disabled { border: 1px solid #646464; background:#646464; }
QTreeView::indicator:indeterminate:disabled { border: 1px solid #646464; }
QTreeView::indicator:indeterminate:focus,
QTreeView::indicator:indeterminate:hover,
QTreeView::indicator:indeterminate:pressed { }

/* Checkbox inside a ListView  --- */
QListView::indicator { height: 12px; width: 12px; border: 1px solid #646464; background:#000; border-radius: 2px; }
QListView::indicator:unchecked { }
QListView::indicator:unchecked:hover { border: 1px solid #284c93; }
QListView::indicator:unchecked:focus { }
QListView::indicator:unchecked:pressed { }
QListView::indicator:unchecked:disabled { border: 1px solid #646464; background:#646464; }
QListView::indicator:checked { image:url(":/images/svg_icons/dark/checkbox_check.svg"); }
QListView::indicator:checked:hover { border: 1px solid #284c93; }
QListView::indicator:checked:focus,
QListView::indicator:checked:pressed {  }
QListView::indicator:checked:disabled { border: 1px solid #646464; background:#646464; }
QListView::indicator:indeterminate:disabled { border: 1px solid #646464; }
QListView::indicator:indeterminate:focus,
QListView::indicator:indeterminate:hover,
QListView::indicator:indeterminate:pressed { }

/* QFrame -----------------------------------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qframe
https://doc.qt.io/qt-5/qframe.html#-prop
https://doc.qt.io/qt-5/qframe.html#details
https://stackoverflow.com/questions/14581498/qt-stylesheet-for-hline-vline-color
--------------------------------------------------------------------------- */
QFrame { border: none; }
QLabel[styleclass="fixFrameBorder"] { }
QFrame[class="tooltip"]
{
  background: #d4d4d4;
  border: 1px solid #1e2023;
  border-radius: 1px;
  color:#000;
}
QFrame[class="tooltip"] QLabel { color:#000; }
QLabel > QFrame::HLine { border: 5px solid red; color: #343434; }

/* QAbstractScrollArea ----------------------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qabstractscrollarea
--------------------------------------------------------------------------- */
QAbstractScrollArea
{
  background: #1E2023;
  border: 1px solid #4a4a4a;
  border-radius: 1px;
  padding: 0px;
  color: #cfcfcf;
}
QAbstractScrollArea:disabled { color: #646464; background: #333538; }

/* --- Workaround for Drawing Sheet Set Properties Dialog --- */
QAbstractScrollArea QWidget[styleclass="scrollAreaWidgetContents"] { background: #2D2F31; }

/* --- Custom Styles for Open File Dialog(Drawing Compare) --- */
QAbstractButton[styleclass="pagebutton"] {
  min-width: 180;
  text-align: left;
  padding: 4px;
  qproperty-iconSize: 20px;
}
QLabel[styleclass="title"] { font-weight: bold; }
QLabel[styleclass="title"] { font-weight: bold; margin: 0px; padding: 0px; }
QAbstractButton[styleclass="pageauxbutton"] { width: 16; height: 16; qproperty-iconSize: 16px; }
QAbstractButton[styleclass="searchauxbutton"] {
  min-width: 16px;
  min-height: 16px;
  max-height: 16px;
  max-width: 16px;
  border: none;
  qproperty-iconSize: 16px;
}
QAbstractButton[styleclass="searchauxbutton"]:disabled { background: none; }
QWidget[styleclass="tooltip"] { border: 1px solid #646464; background: #333538; }

/* --- Licensing Dialog  --- */
QWidget#widgetWelcomeHeader { font-weight: bold; }
QLabel#lbLicenseInfoWithDays { background: #E7D300; }
QLabel#lbNoInternetHome { background: #B82015; color: #FFF; }
QLabel#lbUserName {  }
QLabel#lbUserEmail { font-weight: bold; margin: 2px 0; }
QLabel#lbUserAccountLicenseInfo { background: #00AA00; }
QWidget#CFxDlgLicensing2020 CFxStackWidgetPage { background: #333538; }
QWidget#CFxDlgLicensing2020 QGroupBox#groupBox { background: #333538; border-bottom: 1px solid; }
QWidget#CFxDlgLicensing2020 QGroupBox { margin-top: 0px; }
QWidget#CFxDlgLicensing2020 CFxStackWidgetPage > QScrollArea { border: 0px; }
QWidget#CFxDlgLicensing2020 QAbstractScrollArea QWidget[styleclass="scrollAreaWidgetContents"] { background: #333538; }
